	.equ	LCD_BUSIZE,			(SCR_XSIZE*SCR_YSIZE/2)
	.equ	LCD_ACTIVBUFFER,	0xc300000

	.equ	rLCDCON1,	0x1f00000
	.equ	rLCDCON2,	0x1f00004
	.equ	rLCDCON3,	0x1f00040
	.equ	rLCDSADDR1,	0x1f00008
	.equ	rLCDSADDR2,	0x1f0000c
	.equ	rLCDSADDR3,	0x1f00010
	.equ	rREDLUT,	0x1f00014
	.equ	rGREENLUT,	0x1f00018
	.equ	rBLUELUT,	0x1f0001c
	.equ	rDP1_2,		0x1f00020
	.equ	rDP4_7,		0x1f00024
	.equ	rDP3_5,		0x1f00028
	.equ	rDP2_3,		0x1f0002c
	.equ	rDP5_7,		0x1f00030
	.equ	rDP3_4,		0x1f00034
	.equ	rDP4_5,		0x1f00038
	.equ	rDP6_7,		0x1f0003c
	.equ	rDITHMODE,	0x1f00044

	.equ	CLKVAL_GREY16,	12
	.equ	LINEVAL,		(SCR_YSIZE -1)
	.equ	HOZVAL,			(SCR_XSIZE/4-1)
	.equ	MVAL,			(13)
	.equ	MVAL_USED,		0

	@dimensions of the screen in chars and lines
	.equ	SCR_WIDTH_CHARS,	40
	.equ	SCR_HEIGHT_LINES,	15

	.equ	BIN2INTSIZE,		32
	
	.global	g_auc_Ascii8x16

	.text
	.global	lcd_init
	.global	lcd_drawString
	.global	lcd_clrChar
	.global	lcd_clrLine
	.global	lcd_drawChar
	.global lcd_drawInteger
	.global lcd_clrScreen
	
lcd_init:
	stmfd	sp!,{r0-r5,lr}

	ldr		r0,=rDITHMODE
	ldr		r1,=0x1223a
	str		r1,[r0]			@rDITHMODE=0x1223a;

	ldr		r0,=rDP1_2
	ldr		r1,=0x5a5a
	str		r1,[r0]			@rDP1_2 =0x5a5a;

	ldr		r0,=rDP4_7
	ldr		r1,=0x366cd9b
	str		r1,[r0]			@rDP4_7 =0x366cd9b;

	ldr		r0,=rDP3_5
	ldr		r1,=0xda5a7
	str		r1,[r0]			@rDP3_5 =0xda5a7;
	
	ldr		r0,=rDP2_3
	ldr		r1,=0xad7
	str		r1,[r0]			@rDP2_3 =0xad7;
	
	ldr		r0,=rDP5_7
	ldr		r1,=0xfeda5b7
	str		r1,[r0]			@rDP5_7 =0xfeda5b7;
	
	ldr		r0,=rDP3_4
	ldr		r1,=0xebd7
	str		r1,[r0]			@rDP3_4 =0xebd7;
	
	ldr		r0,=rDP4_5
	ldr		r1,=0xebfd7
	str		r1,[r0]			@rDP4_5 =0xebfd7;
	
	ldr		r0,=rDP6_7
	ldr		r1,=0x7efdfbf
	str		r1,[r0]			@rDP6_7 =0x7efdfbf;

	@#define MVAL_USED 			0
	@rLCDCON1=(0)|(1<<5)|(MVAL_USED<<7)|(0x0<<8)|(0x0<<10)|(CLKVAL_GREY16<<12);
	@#define CLKVAL_GREY16 		(12)
	ldr		r0,=rLCDCON1
	ldr		r1,=(0)|(1<<5)|(0x0<<8)|(0x0<<10)|(CLKVAL_GREY16<<12)
	str		r1,[r0]
	
	@#define LINEVAL				(LCD_YSIZE -1)
	@#define HOZVAL				(LCD_XSIZE/4-1)
	@rLCDCON2=(LINEVAL)|(HOZVAL<<10)|(10<<21); 
	ldr		r0,=rLCDCON2
	ldr		r1,=(LINEVAL)|(HOZVAL<<10)|(10<<21)
	str		r1,[r0]

	@#define M5D(n)				((n) & 0x1fffff)
	@rLCDSADDR1= (0x2<<27) | ( ((LCD_ACTIVE_BUFFER>>22)<<21 ) | M5D(LCD_ACTIVE_BUFFER>>1));
	ldr		r0,=rLCDSADDR1
	ldr		r1,=(0x2<<27)|((LCD_ACTIVBUFFER>>22)<<21)|((LCD_ACTIVBUFFER>>1)&0x1fffff)
	str		r1,[r0]

	@#define MVAL				(13)
	@rLCDSADDR2= M5D(((LCD_ACTIVE_BUFFER+(SCR_XSIZE*LCD_YSIZE/2))>>1)) | (MVAL<<21);
	ldr		r0,=rLCDSADDR2
	ldr		r1,=((((LCD_ACTIVBUFFER+(SCR_XSIZE*SCR_YSIZE/2))>>1))|(MVAL<<21)&0x1fffff)
	str		r1,[r0]

	@rLCDSADDR3= (LCD_XSIZE/4) | ( ((SCR_XSIZE-LCD_XSIZE)/4)<<9 );
	ldr		r0,=rLCDSADDR3
	ldr		r1,=(SCR_XSIZE/4) | ( ((SCR_XSIZE-SCR_XSIZE)/4)<<9 )
	str		r1,[r0]

	@#define MVAL_USED 			0
	@// enable,4B_SNGL_SCAN,WDLY=8clk,WLH=8clk,
	@rLCDCON1=(1)|(1<<5)|(MVAL_USED<<7)|(0x3<<8)|(0x3<<10)|(CLKVAL_GREY16<<12);
	ldr		r0,=rLCDCON1
	ldr		r1,=(1)|(1<<5)|(MVAL_USED<<7)|(0x3<<8)|(0x3<<10)|(CLKVAL_GREY16<<12)
	str		r1,[r0]

	ldr		r0,=rBLUELUT
	ldr		r1,=0xfa40
	str		r1,[r0]			@rBLUELUT=0xfa40;

	@//Enable LCD Logic and EL back-light.
	ldr		r0,=rPDATE
	ldr		r1,[r0]
	and		r1,r1,#0x0e
	str		r1,[r0]			@rPDATE=rPDATE&0x0e;

	bl		lcd_clrScreen
	
	ldmfd	sp!,{r0-r5,pc}

@R0:xpos
@R1:ypos
lcd_clrChar:
	stmfd	sp!,{r2,lr}
	mov		r2,#' '
	bl		lcd_drawChar
	ldmfd	sp!,{r2,pc}

@R0:line# to clear
lcd_clrLine:
	stmfd	sp!,{r0-r2,lr}
	
	mov		r1,r0
	mov		r0,#0
	mov		r2,#SCR_WIDTH_CHARS
lcll05:
	bl		lcd_clrChar
	add		r0,r0,#1
	subs	r2,r2,#1
	bne		lcll05
	
	ldmfd	sp!,{r0-r2,pc}

lcd_clrScreen:
	stmfd	sp!,{r0,lr}
	ldr		r0,=0
lcls05:
	bl		lcd_clrLine
	add		r0,r0,#1
	cmp		r0,#SCR_HEIGHT_LINES
	blt		lcls05
	ldmfd	sp!,{r0,pc}

@Draw a single ascii character on the LCD
@R0:xpos
@R1:ypos
@R2:ascii character to draw
lcd_drawChar:
	stmfd	sp!,{r0-r5,lr}

	add		r1,r1,r1,lsl#2			@mult by 5
	mov		r3,r1,lsl#9				@mult by 32*16 (ypos * 160)
	add		r3,r3,r0,lsl#2
	ldr		r1,=LCD_ACTIVBUFFER
	add		r3,r3,r1
	
	ldr		r1,=g_auc_Ascii8x16
	add		r1,r1,r2,lsl#4

	mov		r2,#16	
ldc05:
	ldrb	r0,[r1],#1
	mov		r4,#8
	mov		r5,#0
ldc10:
	movs	r0,r0,ror#1
	orrcs	r5,r5,#0xf0000000
	mov		r5,r5,lsr#4
	subs	r4,r4,#1
	bne		ldc10
	
	str		r5,[r3]

	add		r3,r3,#160	
	subs	r2,r2,#1
	bne		ldc05
	
	ldmfd	sp!,{r0-r5,pc}

@R0:line
@R1:int to convert
@output - R0:pointer to buffer
lcd_drawInteger:
	stmfd	sp!,{r0-r2,lr}
	mov		r2,r0
	mov		r0,r1
	bl		lcd_bin2char
	
	mov		r1,r0
	mov		r0,r2
	bl		lcd_drawString
	ldmfd	sp!,{r0-r2,pc}

@Draws a string on the LCD
@R0:xpos
@R1:ypos
@R2:ptr to string (NULL terminated)
lcd_drawString:
	stmfd	sp!,{r0-r3,lr}
	mov		r3,r2
lds05:
	ldrb	r2,[r3],#1
	cmp		r2,#0
	beq		lds10
	
	bl		lcd_drawChar
	add		r0,r0,#1
	b		lds05
lds10:
	ldmfd	sp!,{r0-r3,pc}


@R0:number to convert
@ output R0:pointer to string buffer
lcd_bin2char:
	stmfd	sp!,{r1-r3,lr}

	mov		r3,r0
	ldr		r1,=(div10+BIN2INTSIZE-1)
	mov		r2,#0
	strb	r2,[r1],#-1
lbc05:
	mov		r0,r3	
	bl		lcb_mod10
	add		r0,r0,#'0'
	strb	r0,[r1],#-1

	mov		r0,r3	
	bl		lcb_div10
	movs	r3,r0
	bne		lbc05

	add		r0,r1,#1

	ldmfd	sp!,{r1-r3,pc}

@R0:number to divide by 10
@output R0:quoient
lcb_div10:
	stmfd	sp!,{r1-r3,lr}
	mov		r1,r0
	ldr		r3,=0x66666667
	smull	r2,r3,r1,r3
	mov		r2,r3,asr#2
	mov		r3,r1,asr#31
	rsb		r0,r3,r2
	ldmfd	sp!,{r1-r3,pc}

@R0:number to divide by 10
@output R0:remainder
lcb_mod10:
	stmfd	sp!,{r1-r3,lr}

	mov		r1,r0
	ldr		r3,=0x66666667
	smull	r2,r3,r1,r3
	mov		r2,r3,asr#2
	mov		r3,r1,asr#31
	rsb		r2,r3,r2
	mov		r3,r2
	mov		r3,r3,lsl#2
	add		r3,r3,r2
	mov		r3,r3,lsl#1
	rsb		r0,r3,r1

	ldmfd	sp!,{r1-r3,pc}


